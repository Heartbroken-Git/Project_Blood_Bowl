 * @brief Protoype
 */

#include <iostream>
#include <vector>
#include "Team.hpp"
using namespace std;

/*!
*@brief Constructeur des Teams
*@complexite Constante
*/
Team::Team()
{
	i = 0;
	wins = 0;
	losses = 0;
	draws = 0;
}
void Team::setWins(int w)
{
	wins = w;
}
void Team::setLosses(int l)
{
	losses = l;
}
void Team::setDraws(int d)
{
	draws = d;
}
int Team::getWins()
{
	return wins;
}
int Team::getLosses()
{
	return losses;
}
int Team::getDraws()
{
	return draws;
}
void Team::addPlayer(Player p)
{
	roster.push_back(p);
	i++;
}
